pipeline { 
    agent { label 'slave2' }
    
    tools {
        maven 'maven' // Specifies the Maven installation configured in Jenkins
        
    }
    
    stages {
        stage('Compile') {
            steps {
                sh "mvn compile"
            }
        }
        stage('Test') {
            steps {
                sh "mvn test"
            }
        } 
        stage('Package') {
            steps {
                sh "mvn package"
            }
        }        
    }
}
